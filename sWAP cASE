def swap_case(s):
    result=[]
    for letter in s:
        if letter == letter.upper():
            result.append(letter.lower())
        elif letter == letter.lower():
            result.append(letter.upper())
        else:
            result.append(letter)
    return ''.join(result)

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
